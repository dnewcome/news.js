<VirtualHost *:80>
	ServerAdmin webmaster@localhost
	DocumentRoot /var/www

	ProxyPass /newsjs/ http://example.com:3000/
	ProxyPassReverse /newsjs/ http://example.com:3000/
	<Proxy *>
		Order deny,allow
		Allow from all
	</Proxy>

	<Directory />
		Options FollowSymLinks
		AllowOverride None
	</Directory>
</VirtualHost>
